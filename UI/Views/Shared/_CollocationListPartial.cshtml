@using UI.Helpers
@model UI.Models.WcSearchViewModel

<div id="listDiv" class="marginTop3em alignLeft">
	@foreach (var collocation in Model.CollocationList)
	{
		@Html.Partial("_WordListPartial", collocation)
	}
</div>

<div class="pagination">
	@Html.PageLinks(Model.CollocationPagingInfo, x => Url.Action(Model.Action, Model.Controller, new { page = x }))
</div>
